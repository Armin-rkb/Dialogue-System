  <!--DANGANRONPA STORY-->
  <!--
  Characters:
    Monokuma: Judge
    Makoto Naegi: Protaganist
    Chihiro Fujisaki: Victim
    Hifumi Yamada: Suspect
    Kiyotaka Ishimaru: Support
    Kyoko Kirigiri: Culprit
    Byakuya Togami: Suspect
  -->
  <DanganronpaDialogue>

    <Dialogue id="0" type="normal">
      <Line destination="1" portrait="Monokuma" mood="neutral">Alright everyone listen up!</Line>
    </Dialogue>

    <Dialogue id="1" type="normal">
      <Line destination="2" portrait="Monokuma" mood="angry">As you all know there was an incident today.</Line>
    </Dialogue>

    <Dialogue id="2" type="normal">
      <Line destination="3" portrait="Monokuma" mood="laughing">Someone ate Chihiro's cup of noodles ~puh puh puh!</Line>
    </Dialogue>    
      
    <Dialogue id="3" type="normal">
      <Line destination="4" portrait="Kiyotaka Ishimaru" mood="serious">This is no laughing matter!</Line>
    </Dialogue>     
    
    <Dialogue id="4" type="normal">
      <Line destination="5" portrait="Kiyotaka Ishimaru" mood="crossed">We have to find the criminal who took Chihiro's noodles!</Line>
    </Dialogue>
    
    <Dialogue id="5" type="normal">
      <Line destination="6" portrait="Kiyotaka Ishimaru" mood="happy">Now lets solve this, whoever did this please raise your hand!</Line>
    </Dialogue>   
    
    <Dialogue id="6" type="normal">
      <Line destination="7" portrait="Byakuya Togami" mood="backside">This is ridiculous, I'm leaving.</Line>
    </Dialogue>
    
    <Dialogue id="7" type="normal">
      <Line destination="8" portrait="Monokuma" mood="angry">No one is allowed to leave untill one of you finds the culprit!</Line>
    </Dialogue>
       
    <Dialogue id="8" type="normal">
      <Line destination="9" portrait="Monokuma" mood="laughing">I will decide if you picked the right one so good luck! hahahah!</Line>
    </Dialogue>
    
    <Dialogue id="9" type="normal">
      <Line destination="10" portrait="Chihiro Fujisaki" mood="crying">Please guys, it's okay. All I want is an apology.</Line>
    </Dialogue>
    
    <Dialogue id="10" type="normal">
      <Line destination="11" portrait="Byakuya Togami" mood="neutral">*sigh* Fine. We just have to pick the person who did it right?</Line>
    </Dialogue>
    
    <Dialogue id="11" type="normal">
      <Line destination="12" portrait="Byakuya Togami" mood="pointing">Alright, Yamada is the culprit.</Line>
    </Dialogue>
    
    <Dialogue id="12" type="normal">
      <Line destination="13" portrait="Hifumi Yamada" mood="shocked">WHAAAT!?!</Line>
    </Dialogue>

    <Dialogue id="13" type="normal">
      <Line destination="14" portrait="Mondo Owada" mood="neutral">I agree, it was probably him.</Line>
    </Dialogue>

    <Dialogue id="14" type="normal">
      <Line destination="15" portrait="Hifumi Yamada" mood="sad">I didn't do it! Why would you think that..</Line>
    </Dialogue>
    
    <Dialogue id="15" type="normal">
      <Line destination="16" portrait="Byakuya Togami" mood="neutral">...</Line>
    </Dialogue>
    
    <Dialogue id="16" type="normal">
      <Line destination="17" portrait="Mondo Owada" mood="neutral">...</Line>
    </Dialogue>
    
    <Dialogue id="17" type="normal">
      <Line destination="18" portrait="Mondo Owada" mood="thinking">Just a guess.</Line>
    </Dialogue>
    
    <Dialogue id="18" type="normal">
      <Line destination="19" portrait="Makoto Naegi" mood="serious">Stop it guys! We can't just point fingers at people without evidence.</Line>
    </Dialogue>
    
    <Dialogue id="19" type="normal">
      <Line destination="20" portrait="Hifumi Yamada" mood="proud">He's right! And besides, I'm a vegan anyway.</Line>
    </Dialogue>
    
    <Dialogue id="20" type="normal">
      <Line destination="21" portrait="Kyoko Kirigiri" mood="sad">Uhh, yeah okay...</Line>
    </Dialogue>
    
    <Dialogue id="21" type="normal">
      <Line destination="22" portrait="Hifumi Yamada" mood="proud"> I don't eat unhealty food, only HIGH quality fruits and veggies.</Line>
    </Dialogue>
    
    <Dialogue id="22" type="normal">
      <Line destination="23" portrait="Kyoko Kirigiri" mood="neutral">Anyway, this isn't getting us anywhere.</Line>
    </Dialogue>
    
    <Dialogue id="23" type="question">
      <Line portrait="Makoto Naegi" mood="thinking">*Kirigiri is right, I must ask questions to find leads*</Line>
      <Options>
        <Option destination="24" type="normal" portrait="Makoto Naegi" mood="serious" choice="Bluff">I know who the cuplrit is!</Option>
        <Option destination="47" type="normal" portrait="Makoto Naegi" mood="neutral" choice="Ask Chihiro">Hey Chihiro.</Option>
        <Option destination="38" type="normal" portrait="Makoto Naegi" mood="trustworthy" choice="Guess">This must be a setup.</Option>
      </Options>
    </Dialogue>

    <!--Bad Route -->
    <Dialogue id="24" type="normal">
      <Line destination="25" portrait="Chihiro Fujisaki" mood="happy">You do?</Line>
    </Dialogue>

    <Dialogue id="25" type="normal">
      <Line destination="26" portrait="Kyoko Kirigiri" mood="neutral">Ohh? Tell us.</Line>
    </Dialogue>

    <Dialogue id="26" type="normal">
      <Line destination="27" portrait="Makoto Naegi" mood="pointing">The only one who eats a lot of instant meals is Owada.</Line>
    </Dialogue>

    <Dialogue id="27" type="normal">
      <Line destination="28" portrait="Mondo Owada" mood="sad">Thats right.</Line>
    </Dialogue>
    
    <Dialogue id="28" type="normal">
      <Line destination="29" portrait="Makoto Naegi" mood="trustworthy">He was probably hungry and found Chihiro's meal laying around.</Line>
    </Dialogue>
    
    <Dialogue id="29" type="normal">
      <Line destination="30" portrait="Byakuya Togami" mood="neutral">So, got anything to say about this Owada?</Line>
    </Dialogue>
    
    <Dialogue id="30" type="normal">
      <Line destination="31" portrait="Mondo Owada" mood="thinking">Yeah, even though I have known for eating alot of junk...</Line>
    </Dialogue>
    
    <Dialogue id="31" type="normal">
      <Line destination="32" portrait="Mondo Owada" mood="neutral">I wasn't the one that to--</Line>
    </Dialogue>
    
    <Dialogue id="32" type="normal">
      <Line destination="33" portrait="Kiyotaka Ishimaru" mood="serious">OWADA WASN'T THE ONE WHO TOOK IT!</Line>
    </Dialogue>
    
    <Dialogue id="33" type="normal">
      <Line destination="34" portrait="Makoto Naegi" mood="accused">What?!</Line>
    </Dialogue>
    
    <Dialogue id="34" type="normal">
      <Line destination="35" portrait="Kiyotaka Ishimaru" mood="neutral">I was spending the whole day with Owada. It couldn't have been him!</Line>
    </Dialogue>
    
    <Dialogue id="35" type="normal">
      <Line destination="36" portrait="Monokuma" mood="laughing">Puh puh puh!</Line>
    </Dialogue>
    
    <Dialogue id="36" type="normal">
      <Line destination="37" portrait="Monokuma" mood="neutral">Looks like you messed up!</Line>
    </Dialogue>
    
    <Dialogue id="37" type="normal">
      <Line destination="0" portrait="Monokuma" mood="happy">Time to start over!</Line>
    </Dialogue>
    
    <!--Bad Route -->
    <Dialogue id="38" type="normal">
      <Line destination="39" portrait="Kyoko Kirigiri" mood="thinking">What do you mean?</Line>
    </Dialogue>

    <Dialogue id="39" type="normal">
      <Line destination="40" portrait="Makoto Naegi" mood="serious">There probably isn't a cuplrit at all.</Line>
    </Dialogue>

    <Dialogue id="40" type="normal">
      <Line destination="41" portrait="Chihiro Fujisaki" mood="neutral">But, who did it then?</Line>
    </Dialogue>

    <Dialogue id="41" type="normal">
      <Line destination="42" portrait="Makoto Naegi" mood="trustworthy">No one, Monokuma is trying to put us against each other for his enjoyment.</Line>
    </Dialogue>
    
    <Dialogue id="42" type="normal">
      <Line destination="43" portrait="Monokuma" mood="happy">Ahh you really think so Naegi?</Line>
    </Dialogue>

    <Dialogue id="43" type="normal">
      <Line destination="44" portrait="Monokuma" mood="neutral">Although I like playing with you guys, there is a real cuplrit.</Line>
    </Dialogue>
      
    <Dialogue id="44" type="normal">
      <Line destination="45" portrait="Monokuma" mood="laughing">And it is one of you guys ~Puh puh puh!</Line>
    </Dialogue>
    
    <Dialogue id="45" type="normal">
      <Line destination="46" portrait="Makoto Naegi" mood="accused">*Crap I messed up!*</Line>
    </Dialogue>
    
    <Dialogue id="46" type="question">
      <Line portrait="Makoto Naegi" mood="thinking">There still must be another way to find out.</Line>
      <Options>
        <Option destination="24" type="normal" portrait="Makoto Naegi" mood="serious" choice="Bluff">I know who the cuplrit is!</Option>
        <Option destination="47" type="normal" portrait="Makoto Naegi" mood="neutral" choice="Ask Chihiro">Hey Chihiro.</Option>
      </Options>
    </Dialogue>
    
    <!--Good Route -->
    <Dialogue id="47" type="normal">
      <Line destination="48" portrait="Chihiro Fujisaki" mood="neutral">Yes Neagi?</Line>
    </Dialogue>
    
    <Dialogue id="48" type="normal">
      <Line destination="49" portrait="Makoto Naegi" mood="questionig">Could you talk a bit more about how this all began?</Line>
    </Dialogue>
    
    <Dialogue id="49" type="normal">
      <Line destination="50" portrait="Chihiro Fujisaki" mood="thinking">Mhhhh</Line>
    </Dialogue>
  
    <Dialogue id="51" type="normal">
      <Line destination="50" portrait="Chihiro Fujisaki" mood="thinking">---------</Line>
    </Dialogue>

  </DanganronpaDialogue>
</DialogueCollection>
